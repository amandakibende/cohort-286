<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>Anime list</h1>

    <div>
      <div>
        {{#each animes}}
          <div>
            {{this.title}}
          </div>
          <div>
            <img src="{{this.picture}}" alt="" />
          </div>
        {{/each}}
      </div>
    </div>

  </body>
</html>