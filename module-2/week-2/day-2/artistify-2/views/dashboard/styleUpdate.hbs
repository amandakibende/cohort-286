<form method="post" action="/dashboard/style/{{style.id}}" class="form">
    <h1  class="title">style create</h1>
    <label for="name" class="label">name</label>
    <input class="input" type="text" id="name" name="name" value="{{style.name}}">

    <label for="color" class="label">color</label>
    <input type="color" id="color" name="color" value="{{style.color}}">

    <label for="wikiURL" class="label">name</label>
    <input class="input" type="text" id="wikiURL" name="wikiURL" value="{{style.wikiURL}}">

    <button class="btn">create</button>
</form>